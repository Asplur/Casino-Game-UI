    hook.add("PlayerSay", "Command Owner Only", function(msg)
        if not player() == owner() then return end

        local ply = owner()

        --===== GET PLAYER GROUP AND GROUP COLOR =====--

        local player_group = ply:getUserGroup()
        local player_color = team.getColor(ply:getTeam())

        if msg == "h" then

            print(table.toString(Commands[24]), nil, true)

        return ""
        end

        --===== Text =====--

    end)
